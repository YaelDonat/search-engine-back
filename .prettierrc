{
  "tabWidth": 2,
  "quoteProps": "as-needed",
  "singleQuote": true,
  "printWidth": 120,
  "semi": true,
  "useTabs": false,
  "trailingComma": "none",
  "overrides": [
    {
      "files": "*.js",
      "options": {
        "trailingComma": "all"
      }
    }
  ],
  "arrowParens": "avoid",
  "endOfLine": "auto"
}